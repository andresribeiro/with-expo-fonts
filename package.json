{
  "name": "with-expo-fonts",
  "version": "1.0.0",
  "description": "Make your expo apps start faster by loading fonts at build time",
  "main": "app.plugin.js",
  "repository": {
    "type": "git",
    "url": "git@github.com/andresribeiro/with-expo-fonts.git"
  },
  "keywords": [
    "react-native",
    "expo",
    "fonts",
    "startup-time",
    "plugin",
    "plugins",
    "expo-plugins",
    "expo-dev-client",
    "javascript",
    "rn"
  ],
  "author": "Andr√© Ribeiro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andresribeiro/with-expo-fonts/issues"
  },
  "homepage": "https://github.com/andresribeiro/with-expo-fonts#readme",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module"
  },
  "simple-git-hooks": {
    "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
