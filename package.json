  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "cz-emoji": "^1.3.2-canary.2",
    "expo": "^48.0.5",
    "expo-module-scripts": "^3.0.7",
    "rome": "^11.0.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "expo": ">=47.0.0"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    }
  }
}
